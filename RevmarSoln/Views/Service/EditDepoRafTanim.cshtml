@model SahinRektefiyeSoln.Models.DepoRafTanim
@{
	ViewBag.Title = "EditDepoRafTanim";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Action("SetPageHeader", "Admin", new { pageTitle = "Depo Raf Tanımı Düzenle", pagePermission = "XXXXX" })


<div class="w3-panel">
	<div class="w3-row-padding w3-card">
		@using (Html.BeginForm())
		{
			<div class="w3-row">
				<div class="w3-quarter w3-padding">
					<label>Depo Sırası</label>
					@Html.EditorFor(model => model.DepoSirasi, new { htmlAttributes = new { @class = "w3-input w3-border w3-round", maxlength = "5", autocomplete = "off" } })
				</div>
				<div class="w3-quarter w3-padding">
					<label>Depo Rafı</label>
					@Html.EditorFor(model => model.DepoRafi, new { htmlAttributes = new { @class = "w3-input w3-border w3-round", maxlength = "5", autocomplete = "off" } })
				</div>
				<div class="w3-quarter w3-padding">
					<label>Depo Hücresi</label>
					@Html.EditorFor(model => model.DepoHucresi, new { htmlAttributes = new { @class = "w3-input w3-border w3-round", maxlength = "5", autocomplete = "off" } })
				</div>
			</div>
			<div class="w3-threequarter w3-padding">
				@Html.TextAreaFor(model => model.Aciklama, new { @class = "w3-input w3-border w3-round", maxlength = "126" })
			</div>
			<div class="w3-row">
				<div class="w3-quarter">&nbsp;</div>
				<div class="w3-quarter">&nbsp;</div>

				<div class="w3-quarter  w3-padding">
					<button class="w3-btn w3-ripple w3-green w3-right">Kaydet</button>
				</div>
			</div>
		}
	</div>
</div>
